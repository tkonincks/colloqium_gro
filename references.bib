journal = {Physical Review E},
mendeley-groups = {Theory/MCT},
month = aug,
pages = {16},
title = {{Mode-coupling theory of the glass transition for confined fluids}},
url = {http://arxiv.org/abs/1208.1929 http://journals.aps.org/pre/abstract/10.1103/PhysRevE.86.021502},
year = {2012}
}
